    "fs-extra": "^7.0.1",
    "marked": "^0.6.2",
    "open": "^6.2.0",
    "pluralize": "^7.0.0",
    "swagger-parser": "^6.0.5",
    "swagger-ui-express": "^4.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^5.0.5",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.0.0",
    "@types/pluralize": "0.0.29",
